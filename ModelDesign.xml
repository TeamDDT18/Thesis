TargerNameSpace="http://opcfoundation.org/MyServer"
>

<opc:Namespace>
        <opc:Namespace Name="Opcua" Prefix="Opc.Ua" XmlNamespace="http://opcfoundation.org/UA/2008/02/Types.xsd">http://opcfoundation.org/UA/</opc:Namespace>
    <opc:Namespace Name="MyServer" Prefix="MyServer">http://opcfoundation.org/MyServer</opc:Namespace>
</opc:Namespace>

<opc:ObjectType SybolicName="GenericSensorType" BaseType="ua:BaseObjectType">
<opc:Description>A generic sensor that read  process value.</opc:Description>
<opc:Children>
        <opc:Variable SymbolicName="Output" DataType="ua:Double" ValueRank="Scalar" TypeDefinition="ua:AnalogItemType"> </opc:Variable>
        <opc:Property SymbolicName="Units" DataType="ua:String" ValueRank="Scalar" AccessLevel="ReadWrite" />
</opc:Children>
</opc:ObjectType>

<opc:ObjectType SymbolicName="GenericActuatorType" BaseType="ua:BaseObjectType"> 
<opc:Description>Represents a piece of equipment that causes some action to occur.</opc:Description>  
<opc:Children>
    <opc:Variable SymbolicName="Input" DataType="ua:Double" ValueRank="Scalar" TypeDefinition="ua:AnalogItemTyoe" AccessLevel="ReadWrite" />
    <opc:Variable SymbolicName="Output" DataType="ua:Double" ValueRank="Scalar" TypeDefinition="ua:AnalogItemType" AccessLevel="ReadWrite"/>        
</opc:Children>  
</opc:ObjectType>

<opc:ObjectType SymbolicName="GenericMotorType" BaseType="ua:BaseObjectType">
<opc:Description>A generic motor to be monitored.</opc:Description>
<opc:Children>
    <opc:Variable SymbolicName="Speed" DataType="ua:Double" ValueRank="Scalar" TypeDefinition="ua:AnalogItemType" AccessLevel="ReadWrite"/>
    <opc:Variable SymbolicName="Voltage" DataType="ua:Double" ValueRank="Scalar" TypeDefinition="ua:AnalogItemType" AccessLevel="ReadWrite"/>
    <opc:Variable SymbolicName="Current" DataType="ua:Double" ValueRank="Scalar" TypeDefinition="ua:AnalogItemType" AccessLevel="ReadWrite"/>
    <opc:Variable SymbolicName="Power" DataType="ua:Double" ValueRank="Scalar" TypeDefinition="ua:AnalogItemType" AccessLevel="ReadWrite"/>
</opc:Children>
</opc:ObjectType>

<opc:ObjectType SymbolicName="GenericElectronicDevicesType" BaseType="ua:BaseObjectType">
<opc:Description>A generic electronic device to be monitored.</opc:Description>
<opc:Children>
    <opc:Variable SymbolicName="Voltage" DataType="ua:Double" ValueRank="Scalar" TypeDefinition="ua:AnalogItemType" AccessLevel="ReadWrite"/>
    <opc:Variable SymbolicName="Current" DataType="ua:Double" ValueRank="Scalar" TypeDefinition="ua:AnalogItemType" AccessLevel="ReadWrite"/>
    <opc:Variable SymbolicName="Power" DataType="ua:Double" ValueRank="Scalar" TypeDefinition="ua:AnalogItemType" AccessLevel="ReadWrite"/>
</opc:Children>
</opc:ObjectType>

